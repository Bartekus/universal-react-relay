{
  "name": "universal-react-relay",
  "version": "0.2.0",
  "description": "Universal/Isometric React-Relay-GraphQL-/(PostGraphQL)/-PostgreSQL Blog",
  "main": "server",
  "engines": {
    "node": ">=7.8.0",
    "npm": "^4.2.0"
  },
  "author": "Bartek Kus <bartekus@gmail.com> (https://bartekus.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bartekus/universal-react-relay.git"
  },
  "scripts": {
    "schema:1:build": "node server/scripts/buildSchema",
    "schema:2:down": "psql -f server/data/schema-drop.sql",
    "schema:3:up": "psql -f server/data/schema.sql",
    "schema:4:data": "psql -f server/data/data.sql",
    "postinstall": "npm-run-all schema:**",
    "eslint": "eslint . --ext .js",
    "stylelint": "stylelint app/**/*.css",
    "flow": "flow",
    "build": "webpack --env.production -p",
    "test": "jest --config .jestconfig.json",
    "server": "node ./server",
    "start": "concurrently \"babel-node server\" \"npm run update-schema\" \"webpack --watch\"",
    "update-schema": "babel-node server/scripts/updateSchema",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "dependencies": {
    "aphrodite": "^0.5.0",
    "axios": "^0.16.1",
    "babel-polyfill": "^6.22.0",
    "babel-relay-plugin": "^0.11.0",
    "bcrypt": "^0.8.7",
    "concurrently": "^3.4.0",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.2",
    "fs-promise": "^2.0.2",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "graphql-tools": "^0.10.0",
    "isomorphic-relay": "^0.7.4",
    "isomorphic-relay-router": "^0.8.6",
    "jsonwebtoken": "^7.1.7",
    "jwt-decode": "^2.1.0",
    "node-localstorage": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "pg": "^6.1.5",
    "postgraphql": "3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-relay": "^0.10.0",
    "react-relay-network-layer": "^1.3.4",
    "react-router": "^3.0.2",
    "react-router-relay": "^0.13.5",
    "sanitize.css": "^4.1.0",
    "system-sleep": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "casual": "^1.5.10",
    "css-loader": "^0.26.1",
    "ejs": "^2.5.5",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "flow-bin": "^0.38.0",
    "fs": "0.0.1-security",
    "graphql-tools": "^0.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "path": "^0.12.7",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.2.2",
    "webpack": "^2.2.1"
  }
}
